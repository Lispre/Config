bind-key K      prev-unread
bind-key J      next-unread
bind-key p      prev-feed
bind-key n      next-feed
bind-key j      next
bind-key k      prev
bind-key SPACE  open
bind-key h      pageup
bind-key l      pagedown

browser "firefox %u"
html-renderer "w3m -dump -T text/html"
refresh-on-startup yes
external-url-viewer "urlview"
